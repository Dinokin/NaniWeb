@inject SettingsKeeper SettingsKeeper

@if (ViewData["EnableAdsHere"] != null && (bool) ViewData["EnableAdsHere"])
{
    if (bool.Parse(SettingsKeeper.GetSetting("EnableAds").Value))
    {
        <div class="container">
            @Html.Raw(SettingsKeeper.GetSetting("AdsLocationBottom").Value)
        </div>
    }
}
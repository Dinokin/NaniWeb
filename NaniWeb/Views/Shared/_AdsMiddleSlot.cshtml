@inject SettingsKeeper SettingsKeeper

@if (ViewData["EnableAdsHere"] != null && (bool) ViewData["EnableAdsHere"])
{
    if (bool.Parse(SettingsKeeper.GetSetting("EnableAds").Value))
    {
        if (ViewData["DisableAdsContainer"] != null && (bool) ViewData["DisableAdsContainer"])
        {
            <div class="container">
                @Html.Raw(SettingsKeeper.GetSetting("AdsLocationMiddle").Value)
            </div>
        }
        else
        {
            @Html.Raw(SettingsKeeper.GetSetting("AdsLocationMiddle").Value)
        }
    }
}